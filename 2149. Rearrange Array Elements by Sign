class Solution{
    public int[] rearrangeArray(int[] a){
        int[] r = new int[a.length];
        int p = 0, n = 1;
        for (int x : a){
            if(x > 0){
                r[p] = x;
                p += 2;

            }else{
                r[n] = x;
                n += 2;
            }
        }
        return r;
    }
}

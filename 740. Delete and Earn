class Solution {
    public int deleteAndEarn(int[] nums) {
        int[] sum = new int[10001];
        for(int n : nums) sum[n] += n;

        int prev = 0, curr = 0;
        for (int i = 0; i< 10001; i++) {
            int temp = curr;
            curr = Math.max(curr, prev + sum[i]);
            prev = temp;
        }
        return curr;
    }
}
